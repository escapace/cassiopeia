  "scripts": {
    "build": "node ../../scripts/build.mjs",
    "deploy": "pnpm deploy --filter '.' lib/context",
    "lint": "eslint 'src/**/*.ts'",
    "test": "node ../../scripts/test.mjs && c8 mocha --enable-source-maps 'lib/tests/**/*.spec.js'",
    "typecheck": "tsc --noEmit"
