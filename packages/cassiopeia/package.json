  "devDependencies": {
    "@types/chai": "4.3.14",
    "@types/mocha": "10.0.6",
    "@types/node": "20.12.3",
    "c8": "9.1.0",
    "chai": "5.1.0",
    "lint-staged": "15.2.2",
    "mocha": "10.4.0",
    "prettier": "3.2.5",
    "prettier-config-escapace": "1.2.0",
